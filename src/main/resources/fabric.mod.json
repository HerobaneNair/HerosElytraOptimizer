  "authors": [
    "HerobaneNair",
    "HerobaneNair (again)"
    ],
  "contact": {
    "sources": "https://github.com/herobane/HerosElytraOptimizer"
  },
  "license": "MIT",
  "icon": "assets/heroselytraoptimizer/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "hero.bane.HerosElytraOptimizerClient"
    ]
  },
  "mixins": [
    {
      "config": "heroselytraoptimizer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "fabric": "*",
    "minecraft": "~1.21"
  }
}